<form title="New Call" 
	  class="panel"
	  id="SugarCall_new_form" 
	  method="POST" 
	  action="<%=url_for('create')%>">
	<fieldset>
		<input type="hidden" name="id" value="<%=@SugarCall.object%>"/>
		
		  <div class="row">
			  <label>name: </label>
			  <input type="text" name="SugarCall[name]"/>
		  </div>
		
		  <div class="row">
			  <label>date_entered: </label>
			  <input type="text" name="SugarCall[date_entered]"/>
		  </div>
		
		  <div class="row">
			  <label>date_modified: </label>
			  <input type="text" name="SugarCall[date_modified]"/>
		  </div>
		
		  <div class="row">
			  <label>modified_by_name: </label>
			  <input type="text" name="SugarCall[modified_by_name]"/>
		  </div>
		
		  <div class="row">
			  <label>created_by_name: </label>
			  <input type="text" name="SugarCall[created_by_name]"/>
		  </div>
		
		  <div class="row">
			  <label>description: </label>
			  <input type="text" name="SugarCall[description]"/>
		  </div>
		
		  <div class="row">
			  <label>assigned_user_name: </label>
			  <input type="text" name="SugarCall[assigned_user_name]"/>
		  </div>
		
		  <div class="row">
			  <label>duration_hours: </label>
			  <input type="text" name="SugarCall[duration_hours]"/>
		  </div>
		
		  <div class="row">
			  <label>duration_minutes: </label>
			  <input type="text" name="SugarCall[duration_minutes]"/>
		  </div>
		
		  <div class="row">
			  <label>date_start: </label>
			  <input type="text" name="SugarCall[date_start]"/>
		  </div>
		
		  <div class="row">
			  <label>date_end: </label>
			  <input type="text" name="SugarCall[date_end]"/>
		  </div>
		
		  <div class="row">
			  <label>status: </label>
			  <input type="text" name="SugarCall[status]"/>
		  </div>
		
		  <div class="row">
			  <label>direction: </label>
			  <input type="text" name="SugarCall[direction]"/>
		  </div>
		
		  <div class="row">
			  <label>reminder_time: </label>
			  <input type="text" name="SugarCall[reminder_time]"/>
		  </div>
		
		  <div class="row">
			  <label>contact_name: </label>
			  <input type="text" name="SugarCall[contact_name]"/>
		  </div>
		
	</fieldset>
	<input type="submit" value="Create"/>
</form>