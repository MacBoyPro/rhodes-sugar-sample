<form title="Edit Call" 
	  class="panel" 
	  id="SugarCall_edit_form" 
	  method="POST" 
	  action="<%=url_for('update')%>">
	<fieldset>
		<input type="hidden" name="id" value="<%=@SugarCall.object%>"/>
		
		  <div class="row">
			  <label>name: </label>
			  <input type="text" name="SugarCall[name]" value="<%=@SugarCall.name%>"/>
		  </div>
		
		  <div class="row">
			  <label>date_entered: </label>
			  <input type="text" name="SugarCall[date_entered]" value="<%=@SugarCall.date_entered%>"/>
		  </div>
		
		  <div class="row">
			  <label>date_modified: </label>
			  <input type="text" name="SugarCall[date_modified]" value="<%=@SugarCall.date_modified%>"/>
		  </div>
		
		  <div class="row">
			  <label>modified_by_name: </label>
			  <input type="text" name="SugarCall[modified_by_name]" value="<%=@SugarCall.modified_by_name%>"/>
		  </div>
		
		  <div class="row">
			  <label>created_by_name: </label>
			  <input type="text" name="SugarCall[created_by_name]" value="<%=@SugarCall.created_by_name%>"/>
		  </div>
		
		  <div class="row">
			  <label>description: </label>
			  <input type="text" name="SugarCall[description]" value="<%=@SugarCall.description%>"/>
		  </div>
		
		  <div class="row">
			  <label>assigned_user_name: </label>
			  <input type="text" name="SugarCall[assigned_user_name]" value="<%=@SugarCall.assigned_user_name%>"/>
		  </div>
		
		  <div class="row">
			  <label>duration_hours: </label>
			  <input type="text" name="SugarCall[duration_hours]" value="<%=@SugarCall.duration_hours%>"/>
		  </div>
		
		  <div class="row">
			  <label>duration_minutes: </label>
			  <input type="text" name="SugarCall[duration_minutes]" value="<%=@SugarCall.duration_minutes%>"/>
		  </div>
		
		  <div class="row">
			  <label>date_start: </label>
			  <input type="text" name="SugarCall[date_start]" value="<%=@SugarCall.date_start%>"/>
		  </div>
		
		  <div class="row">
			  <label>date_end: </label>
			  <input type="text" name="SugarCall[date_end]" value="<%=@SugarCall.date_end%>"/>
		  </div>
		
		  <div class="row">
			  <label>status: </label>
			  <input type="text" name="SugarCall[status]" value="<%=@SugarCall.status%>"/>
		  </div>
		
		  <div class="row">
			  <label>direction: </label>
			  <input type="text" name="SugarCall[direction]" value="<%=@SugarCall.direction%>"/>
		  </div>
		
		  <div class="row">
			  <label>reminder_time: </label>
			  <input type="text" name="SugarCall[reminder_time]" value="<%=@SugarCall.reminder_time%>"/>
		  </div>
		
		  <div class="row">
			  <label>contact_name: </label>
			  <input type="text" name="SugarCall[contact_name]" value="<%=@SugarCall.contact_name%>"/>
		  </div>
		
	</fieldset>
	<input type="submit" value="Update"/>
	<p align="center">
		<a href="<%=url_for('delete', @SugarCall.object)%>">
			<font color="#990000">Delete</font>
		</a>
	</p>
</form>